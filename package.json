{
  "name": "ufiber",
  "version": "0.1.0-alpha.0",
  "type": "module",
  "description": "next-gen webserver for node-js developer",
  "main": "./dist/index.d.cjs",
  "module": "./dist/index.d.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin/install.js"
  ],
  "scripts": {
    "build": "pnpm remove-dist && node build.ts",
    "build:watch": "pnpm remove-dist && node build.ts --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,js}\" --fix",
    "test": "vitest run",
    "format": "prettier --write \"./{src,tests}/**/*.{ts,js}\"",
    "release": "pnpm run build && npm publish",
    "pre-commit": "pnpm format && pnpm test",
    "check-types": "tsc --noEmit",
    "coverage": "vitest --run --coverage",
    "remove-dist": "rm -rf dist",
    "prepare": "husky",
    "postinstall": "node bin/install.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs"
    },
    "./consts": {
      "import": "./dist/consts.js",
      "types": "./dist/consts.d.ts",
      "require": "./dist/consts.cjs"
    },
    "./uws": {
      "import": "./uws/ESM_wrapper.mjs",
      "types": "./uws/index.d.ts",
      "require": "./uws/uws.js"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts",
      "require": "./dist/core/index.cjs"
    },
    "./tools": {
      "import": "./dist/utils/tools/index.js",
      "types": "./dist/utils/tools/index.d.ts",
      "require": "./dist/utils/tools/index.cjs"
    },
    "./logger": {
      "import": "./dist/middle/logger.js",
      "types": "./dist/middle/logger.d.ts",
      "require": "./dist/middle/logger.cjs"
    },
    "./cors": {
      "import": "./dist/middle/cors.js",
      "types": "./dist/middle/cors.d.ts",
      "require": "./dist/middle/cors.cjs"
    },
    "./proxy": {
      "import": "./dist/middle/proxy.js",
      "types": "./dist/middle/proxy.d.ts",
      "require": "./dist/middle/proxy.cjs"
    },
    "./stream": {
      "import": "./dist/helps/stream/index.js",
      "types": "./dist/helps/stream/index.d.ts",
      "require": "./dist/helps/stream/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "uws": [
        "./uws/index.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ],
      "consts": [
        "./dist/consts.d.ts"
      ],
      "core": [
        "./dist/core/index.d.ts"
      ],
      "cors": [
        "./dist/middle/cors.d.ts"
      ],
      "proxy": [
        "./dist/middle/proxy.d.ts"
      ],
      "logger": [
        "./dist/middle/logger.d.ts"
      ],
      "tools": [
        "./dist/utils/tools/index.d.ts"
      ],
      "stream": [
        "./dist/helps/stream/index.d.ts"
      ]
    }
  },
  "author": "Aashish Panchal <aipanchal51@gmail.com>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vajra-Labs/ufiber.git"
  },
  "homepage": "https://github.com/Vajra-Labs/ufiber#readme",
  "keywords": [
    "node",
    "http",
    "web",
    "server",
    "uws",
    "ufiber"
  ],
  "dependencies": {
    "ansis": "^4.2.0",
    "mrmime": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@swc/core": "^1.15.3",
    "@types/node": "^24.10.2",
    "arg": "^5.0.2",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsdown": "^0.15.12",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "unplugin-swc": "^1.5.9",
    "unplugin-unused": "^0.5.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.25.0"
}
